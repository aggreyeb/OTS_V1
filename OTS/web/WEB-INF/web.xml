<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>PingServlet</servlet-name>
        <servlet-class>OTS.Servlets.PingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AuthenticationServlet</servlet-name>
        <servlet-class>OTS.Servlets.AuthenticationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>KnowledgeMapServlet</servlet-name>
        <servlet-class>OTS.Servlets.KnowledgeMapServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CourseServlet</servlet-name>
        <servlet-class>OTS.Servlets.CourseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestGenerationServlet</servlet-name>
        <servlet-class>OTS.Servlets.TestGenerationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LookupListServlet</servlet-name>
        <servlet-class>OTS.Servlets.LookupListServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserManagementServlet</servlet-name>
        <servlet-class>OTS.Servlets.UserManagementServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestQuestionBankServlet</servlet-name>
        <servlet-class>OTS.Servlets.TestQuestionBankServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>PingServlet</servlet-name>
        <url-pattern>/PingServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuthenticationServlet</servlet-name>
        <url-pattern>/AuthenticationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>KnowledgeMapServlet</servlet-name>
        <url-pattern>/KnowledgeMapServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CourseServlet</servlet-name>
        <url-pattern>/CourseServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestGenerationServlet</servlet-name>
        <url-pattern>/TestGenerationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LookupListServlet</servlet-name>
        <url-pattern>/LookupListServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserManagementServlet</servlet-name>
        <url-pattern>/UserManagementServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestQuestionBankServlet</servlet-name>
        <url-pattern>/TestQuestionBankServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    
    
</web-app>
